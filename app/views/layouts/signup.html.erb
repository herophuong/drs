<!DOCTYPE html>
<html>
<head>
  <title>Daily Report System</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body class="active_admin logged_out new">
<div id="wrapper">
    <div id="content_wrapper">
        <div id="active_admin_content">
            <div id="login">
                <h2>Daily Report System Sign Up</h2>

                <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { class: "formtastic admin_user" }) do |f| %>
                <%= devise_error_messages! %>

                <div><%= f.label :email %><br />
                <%= f.email_field :email, :autofocus => true %></div>

                <br /><br />
                <div><%= f.submit "Sign up" %></div>
                <% end %>

                <%= render "devise/shared/links" %>
            </div>
        </div>
    </div>
            
</div>
</body>
</html>


