<h1>Listing reports</h1>

<table>
  <tr>
    <th>Report title</th>
    <th>Content</th>
    <th>Filelink</th>
    <th>Time</th>
    <th>Admin user</th>
    <th>Day</th>
    <th>Week</th>
    <th>Month</th>
    <th>Year</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reports.each do |report| %>
  <tr>
    <td><%= report.report_title_id %></td>
    <td><%= report.content %></td>
    <td><%= report.fileLink %></td>
    <td><%= report.time %></td>
    <td><%= report.admin_user_id %></td>
    <td><%= report.day %></td>
    <td><%= report.week %></td>
    <td><%= report.month %></td>
    <td><%= report.year %></td>
    <td><%= link_to 'Show', report %></td>
    <td><%= link_to 'Edit', edit_report_path(report) %></td>
    <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Report', new_report_path %>
